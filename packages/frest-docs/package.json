{
  "private": true,
  "name": "frest-docs",
  "description": "Frest documentation site using hugo",
  "version": "0.10.0",
  "license": "Apache-2.0",
  "scripts": {
    "doc": "run-s del:p doc:d",
    "doc:d": "hugo && echo frest.surge.sh > public/CNAME",
    "del": "run-p del:*",
    "del:a": "del-cli static/api",
    "del:p": "del-cli public",
    "serve": "hugo server -w"
  },
  "devDependencies": {
    "del-cli": "^1.1.0",
    "frest": "^0.10.0",
    "frest-auth": "^0.10.0",
    "frest-json": "^0.10.0",
    "hugo-bin": "^0.29.0"
  }
}
