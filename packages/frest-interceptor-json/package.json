		"frest": "~0.5.0"
	},
	"devDependencies": {
		"@types/fetch-mock": "^5.12.2",
		"@types/node": "8.9.1",
		"@types/sinon": "^4.1.3",
		"@types/webpack": "^3.8.7",
		"ava": "^0.25.0",
		"browser-env": "^3.2.5",
		"cross-env": "^5.1.3",
		"del-cli": "^1.1.0",
		"faucet": "^0.0.1",
		"fetch-mock": "^6.0.0",
		"frest": "^0.5.0",
		"node-fetch": "^2.0.0",
		"npm-run-all": "^4.1.2",
		"nyc": "^11.4.1",
		"sinon": "^4.3.0",
		"ts-loader": "^3.5.0",
		"tslint": "^5.9.1",
