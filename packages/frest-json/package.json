{
  "name": "frest-json",
  "version": "0.4.0",
  "description": "A set of JSON interceptors for Frest",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "umd/frest-json.umd.js",
  "typings": "lib/index.d.ts",
  "homepage": "https://github.com/panjiesw/frest",
  "repository": {
    "type": "git",
    "url": "https://github.com/panjiesw/frest/tree/master/packages/frest-json"
  },
  "author": {
    "name": "Panjie Setiawan Wicaksono",
    "email": "panjie@panjiesw.com",
    "url": "http://panjiesw.life"
  },
  "bugs": {
    "url": "https://github.com/panjiesw/frest/issues"
  },
  "keywords": [
    "rest",
    "client",
    "browser",
    "fetch",
    "whatwg-fetch",
    "frest",
    "json",
    "interceptor"
  ],
  "files": [
    "es/",
    "lib/",
    "umd/"
  ],
  "scripts": {
    "b:es": "tsc --declaration --outDir es",
    "b:lib": "tsc --declaration -m commonjs --outDir lib",
    "b:umd": "webpack --profile",
    "build": "npm-run-all lint del --parallel b:*",
    "d:b": "del-cli es/**/__tests__ lib/**/__tests__ es/**/*.map lib/**/*.map",
    "d:unit": "del-cli lib .nyc_output coverage",
    "del": "del-cli es lib umd .nyc_output coverage node_modules/.cache",
    "lint": "tslint -c ../../tslint.json -p tsconfig.json",
    "prep": "yarn b:lib --sourceMap",
    "report": "nyc report",
    "start": "run-p w:lib w:a",
    "t:a": "cross-env NODE_ENV=test nyc ava --tap | faucet",
    "test": "run-s unit report",
    "unit": "run-s d:unit 'b:lib --sourceMap' t:a",
    "w:a": "ava --watch",
    "w:lib": "yarn b:lib -w --preserveWatchOutput"
  },
  "ava": {
    "concurrency": 5,
    "files": [
      "lib/__tests__/**/*.js"
    ],
    "require": [
      "frest/src/__tests__/_helpers.js"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov",
      "cobertura"
    ]
  },
  "peerDependencies": {
    "frest": "~0.6.0"
  },
  "devDependencies": {
    "@types/fetch-mock": "^6.0.1",
    "@types/node": "8.9.1",
    "@types/sinon": "^4.3.1",
    "@types/webpack": "^4.1.3",
    "ava": "^1.0.0-beta.3",
    "browser-env": "^3.2.5",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "faucet": "^0.0.1",
    "fetch-mock": "^6.3.0",
    "frest": "^0.7.0",
    "node-fetch": "^2.1.2",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "sinon": "^4.5.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
