{
  "extends": "../../config/tsconfig.base",
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "es",
    "baseUrl": ".",
    "composite": true
  },
  "include": [
    "src"
  ],
  "typedocOptions": {
    "exclude": "**/__tests__/**",
    "out": "../../docs/api/frest",
    "excludePrivate": true,
    "excludeExternals": true,
    "externalPattern": "**/node_modules/**",
    "includeDeclarations": false
  }
}
