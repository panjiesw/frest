{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "src"
  },
  "exclude": [
    "node_modules",
    "es",
    "lib",
    "umd"
  ],
  "typedocOptions": {
    "exclude": "**/__tests__/**",
    "out": "../frest-docs/static/api/frest",
    "excludePrivate": true,
    "excludeExternals": true,
    "externalPattern": "**/node_modules/**",
    "includeDeclarations": false
  }
}
