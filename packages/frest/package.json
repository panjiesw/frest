{
  "name": "frest",
  "version": "0.6.1",
  "description": "REST client for browser with Fetch",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "umd/frest.umd.js",
  "typings": "lib/index.d.ts",
  "homepage": "https://github.com/panjiesw/frest",
  "repository": {
    "type": "git",
    "url": "https://github.com/panjiesw/frest/tree/master/packages/frest"
  },
  "author": {
    "name": "Panjie Setiawan Wicaksono",
    "email": "panjie@panjiesw.com",
    "url": "http://panjiesw.life"
  },
  "bugs": {
    "url": "https://github.com/panjiesw/frest/issues"
  },
  "keywords": [
    "rest",
    "client",
    "browser",
    "fetch",
    "whatwg-fetch"
  ],
  "files": [
    "es/",
    "lib/",
    "umd/"
  ],
  "scripts": {
    "b:es": "tsc --declaration --outDir es",
    "b:lib": "tsc --declaration -m commonjs --outDir lib",
    "b:umd": "webpack --profile",
    "build": "run-s lint del b:* d:b",
    "d:b": "del-cli es/**/__tests__ lib/**/__tests__",
    "d:unit": "del-cli lib .nyc_output coverage",
    "del": "del-cli es lib umd .nyc_output coverage node_modules/.cache",
    "lint": "tslint -p tsconfig.json",
    "report": "nyc report",
    "start": "run-p w:lib w:a",
    "t:a": "cross-env NODE_ENV=test nyc ava --tap | faucet",
    "test": "run-s unit report",
    "unit": "run-s d:unit b:lib t:a",
    "w:a": "ava --watch",
    "w:lib": "yarn b:lib -w"
  },
  "ava": {
    "concurrency": 5,
    "files": [
      "lib/__tests__/**/*.js"
    ],
    "require": [
      "./src/__tests__/_helpers.js"
    ]
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov",
      "cobertura"
    ]
  },
  "devDependencies": {
    "@types/fetch-mock": "^6.0.0",
    "@types/node": "8.9.1",
    "@types/object-assign": "^4.0.30",
    "@types/qs": "^6.5.1",
    "@types/query-string": "^5.0.1",
    "@types/sinon": "^4.1.3",
    "@types/webpack": "^3.8.7",
    "ava": "^1.0.0-beta.3",
    "browser-env": "^3.2.5",
    "cross-env": "^5.1.3",
    "del-cli": "^1.1.0",
    "faucet": "^0.0.1",
    "fetch-mock": "^6.0.0",
    "form-data": "^2.3.1",
    "node-fetch": "^2.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "sinon": "^4.3.0",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "query-string": "^5.1.0",
    "tslib": "^1.9.0"
  },
  "optionalDependencies": {
    "whatwg-fetch": "^2.0.3"
  }
}
