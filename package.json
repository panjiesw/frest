{
	"name": "frest",
	"version": "0.3.0-dev.0",
	"description": "REST client for browser with Fetch",
	"files": [
		"es6",
		"lib"
	],
	"license": "MIT",
	"main": "lib/index",
	"module": "es6/index",
	"jsnext:main": "es6/index",
	"types": "lib/index.d.ts",
	"homepage": "https://github.com/panjiesw/frest",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/panjiesw/frest.git"
	},
	"author": {
		"name": "Panjie Setiawan Wicaksono",
		"email": "panjie@panjiesw.com",
		"url": "http://panjiesw.life"
	},
	"bugs": {
		"url": "https://github.com/panjiesw/frest/issues"
	},
	"keywords": [
		"rest",
		"client",
		"browser",
		"fetch",
		"whatwg-fetch"
	],
	"scripts": {
		"build": "npm run build:es6 && npm run build:es5",
		"build:es6": "tsc -p tsconfig.build.json",
		"build:es5": "babel es6 -d lib",
		"build:umd": "tsc -p tsconfig.build.json --module umd --outFile umd.js",
		"clean": "del es6 lib",
		"clean:cache": "del .awcache",
		"codecov": "codecov",
		"coverage": "npm run coverage:remap && npm run coverage:report",
		"coverage:remap": "remap-istanbul -i coverage/coverage.json -o coverage/coverage-remapped.json -t json -e node_modules",
		"coverage:report": "node tools/coverage.js",
		"lint": "tslint -e './node_modules/**/*' -e './lib/**/*' -e './es6/**/*' -e './**/*.d.ts' './**/*.ts'",
		"test": "cross-env NODE_ENV=test COVER=true karma start",
		"watch": "cross-env NODE_ENV=test karma start --auto-watch --no-single-run",
		"prebuild": "npm run lint && npm run clean",
		"postbuild": "cpx './es6/**/*.d.ts' './lib'",
		"posttest": "npm run coverage"
	},
	"optionalDependencies": {
		"whatwg-fetch": "^2.0.1"
	},
	"devDependencies": {
		"@types/deep-assign": "^0.1.0",
		"@types/fetch-mock": "^5.8.0",
		"@types/jasmine": "^2.5.41",
		"@types/karma": "^0.13.33",
		"@types/node": "^7.0.0",
		"@types/object-assign": "^4.0.30",
		"@types/query-string": "^3.0.31",
		"@types/webpack": "^2.2.2",
		"@types/whatwg-fetch": "^0.0.33",
		"awesome-typescript-loader": "^3.0.0-beta.18",
		"babel-cli": "^6.18.0",
		"babel-preset-env": "^1.1.8",
		"codecov": "^1.0.1",
		"core-js": "^2.4.1",
		"cpx": "^1.5.0",
		"cross-env": "^3.1.4",
		"del-cli": "^0.2.1",
		"fetch-mock": "^5.8.1",
		"istanbul": "^0.4.5",
		"jasmine-core": "^2.5.2",
		"karma": "^1.4.0",
		"karma-cli": "^1.0.1",
		"karma-coverage": "^1.1.1",
		"karma-jasmine": "^1.1.0",
		"karma-phantomjs-launcher": "^1.0.2",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-sourcemap-writer": "^0.1.2",
		"karma-spec-reporter": "^0.0.26",
		"karma-webpack": "^2.0.1",
		"remap-istanbul": "^0.8.4",
		"tslint": "^4.3.1",
		"typescript": "^2.1.5",
		"webpack": "^2.2.0"
	},
	"dependencies": {
		"deep-assign": "^2.0.0",
		"object-assign": "^4.1.0",
		"query-string": "^4.3.1"
	}
}
