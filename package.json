{
	"name": "frest",
	"version": "0.1.0-dev.0",
	"description": "REST client for browser with Fetch",
	"files": [
		"es6",
		"lib"
	],
	"license": "MIT",
	"main": "lib/index",
	"module": "es6/index",
	"jsnext:main": "es6/index",
	"types": "lib/index.d.ts",
	"homepage": "https://github.com/panjiesw/frest",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/panjiesw/frest.git"
	},
	"author": {
		"name": "Panjie Setiawan Wicaksono",
		"email": "panjie@panjiesw.com",
		"url": "http://panjiesw.life"
	},
	"bugs": {
		"url": "https://github.com/panjiesw/frest/issues"
	},
	"keywords": [
		"rest",
		"client",
		"browser",
		"fetch",
		"whatwg-fetch"
	],
	"scripts": {
		"build": "npm run build:es6 && npm run build:es5",
		"build:es6": "tsc --declaration --removeComments",
		"build:es5": "babel es6 -d lib",
		"clean": "del es6 lib",
		"prebuild": "npm run clean",
		"postbuild": "cpx './es6/**/*.d.ts' './lib'"
	},
	"optionalDependencies": {
		"whatwg-fetch": "^2.0.1"
	},
	"devDependencies": {
		"@types/deep-assign": "^0.1.0",
		"@types/fetch-mock": "^5.8.0",
		"@types/node": "^7.0.0",
		"@types/object-assign": "^4.0.30",
		"@types/query-string": "^3.0.31",
		"@types/whatwg-fetch": "^0.0.33",
		"babel-cli": "^6.18.0",
		"babel-preset-env": "^1.1.8",
		"cpx": "^1.5.0",
		"del-cli": "^0.2.1",
		"fetch-mock": "^5.8.1",
		"tslint": "^4.3.1",
		"typescript": "^2.1.5"
	},
	"dependencies": {
		"deep-assign": "^2.0.0",
		"object-assign": "^4.1.0",
		"query-string": "^4.3.1"
	}
}
